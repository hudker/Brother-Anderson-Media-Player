<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <set>
            <objectAnimator android:duration="200" android:valueTo="12.0dip" android:valueType="floatType" android:propertyName="elevation" />
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:duration="200" android:valueTo="1.0dip" android:valueType="floatType" android:propertyName="elevation" />
        </set>
    </item>
</selector>